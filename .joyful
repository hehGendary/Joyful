func noAsnwer(str) {
    for (i = 0, i < strlen(str), i = i + 1) {
        if (!(char(str, i) == "|")) return true
    }
    return false
}

func valid(str) {
    for (i = 0, i < strlen(str), i = i + 1) {
        isValid = false
        for (j = 0, j < arrlen(alphabet), j = j + 1) {
            if (char(str, i) == alphabet[j]) {
                isValid = true
            }
        }
        if (!isValid) return false
    }
    return true
}

