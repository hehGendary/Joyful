func add(arr, number) {
    oldlen = arrlen(arr)
    newarray = array(oldlen + 1)
    for (i = 0, i < oldlen, i = i + 1) {
        newarray[i] = arr[i]
    }
    newarray[oldlen] = number
    return newarray
}

